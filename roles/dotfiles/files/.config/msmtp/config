account work
from ahmed.elgabri@lightspeedhq.com
user ahmed.elgabri@lightspeedhq.com
passwordeval ~/.dotfiles/extra/bin/get-keychain-pass ahmed.elgabri+mutt@lightspeedhq.com gmail.com

account default : personal
